{ "name": "@vigilio/express",
"version": "1.0.0",
"description": "create web apps in server ",
"bin": "./bin/cli.js",    
"author": "Vigilio",
"license": "ISC",
"scripts": {
    "start:dev": "cross-env NODE_ENV=development  ts-node-dev --respawn -r tsconfig-paths/register  src/main.ts",
    "build": "rm -rf build && tsc --project tsconfig.build.json",
    "start": "cross-env NODE_ENV=development node build/main.js",
    "test": "vitest",
    "rome:format": "rome format  ./src --write",
    "rome:check": "rome check ./src"
},
"keywords": [
    "@vigilio/express",
    "@vigilio/express-api"
]}